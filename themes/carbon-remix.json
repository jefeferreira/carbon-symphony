  "tokenColors": [
    {
      "settings": {
        "foreground": "#B392F0"
      }
    },
    {
      "scope": [
        "keyword.operator.accessor",
        "meta.group.braces.round.function.arguments",
        "markup.fenced_code meta.embedded.block"
      ],
      "settings": {
        "foreground": "#D1D1D1"
      }
    },
    {
      "scope": ["punctuation.terminator.statement"],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "meta.block",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#F6E466"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#C39DF9"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["strong", "markup.heading.markdown", "markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C39DF9"
      }
    },
    {
      "scope": ["markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#FF6274"
      }
    },

    {
      "scope": ["string", "markup.fenced_code", "markup.inline"],
      "settings": {
        "foreground": "#9DB1C5"
      }
    },
    {
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#7F8285"
      }
    },

    {
      "scope": "variable",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },

    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#91D0FF"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#F8818E"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#8DF77C"
      }
    },

    {
      "scope": [
        "constant.language",
        "variable.other.class",
        "meta.property-name",
        "support",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#FF6274"
      }
    },

    {
      "scope": [
        "constant.numeric",
        "constant.other.placeholder",
        "meta.property-value",
        "keyword.other.unit",
        "keyword.other.template",
        "entity.name.tag.yaml",
        "entity.other.attribute-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8DF77C"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "storage.control.clojure",
        "entity.name.function.clojure",
        "support.function.node"
      ],
      "settings": {
        "foreground": "#FF86CB"
      }
    },
    {
      "scope": "constant.character.format.placeholder",
      "settings": {
        "foreground": "#F6E466"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "entity.name.function.member",
      "settings": {
        "foreground": "#C39DF9"
      }
    },
    {
      "scope": "variable.language.special.self",
      "settings": {
        "foreground": "#91D0FF"
      }
    },
    {
      "scope": [
        "meta.function-call.generic.python",
        "meta.function-call.python",
        "meta.member.access.python",
        "source.python"
      ],
      "settings": {
        "foreground": "#C39DF9"
      }
    },

    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "meta.function-call",
        "meta.instance.constructor",
        "entity.other.attribute-name",
        "entity.name.function",
        "constant.keyword.clojure"
      ],
      "settings": {
        "foreground": "#C39DF9"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "string.quoted",
        "string.regexp",
        "string.interpolated",
        "string.template",
        "string.unquoted.plain.out.yaml",
        "keyword.other.template"
      ],
      "settings": {
        "foreground": "#FF95A1"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316BCD"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.definition.dict",
        "meta.function"
      ],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": ["punctuation.definition.parameters"],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": ["punctuation.definition.arguments"],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": ["punctuation.accessor", "meta.function-call", "source"],

      "settings": {
        "foreground": "#E2E2E2"
      }
    },

    {
      "scope": "meta.brace.square",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "meta.brace.round",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#91D0FF"
      }
    },
    {
      "scope": "support.variable.property",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },

    {
      "scope": "support.constant.property",
      "settings": {
        "foreground": "#FFB86C"
      }
    },

    {
      "scope": [
        "punctuation.definition.tag.begin.html",
        "meta.tag.structure.html.end.html",
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FFB86C"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8DF77C"
      }
    },

    {
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#C39DF9"
      }
    },

    {
      "scope": "meta.function-call.arguments",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "scope": "source.python",
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "name": "[Custom] Markdown links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#8EC3FF"
      }
    },
    {
      "name": "[Custom] Markdown list",
      "scope": ["beginning.punctuation.definition.list.markdown"],
      "settings": {
        "foreground": "#FF7A84"
      }
    },
    {
      "name": "[Custom] Markdown punctuation definition",
      "scope": "punctuation.definition.metadata.markdown",
      "settings": {
        "foreground": "#5FAAFF"
      }
    },
    {
      "name": "[Custom] Markdown punctuation definition brackets",
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#79B8FF"
      }
    }
  ],
  "semanticHighlighting": true
}
